.wrapper
  .header
    //%input.search_field{name: "category",placeholder: "検索する", :type => "email", :value => ""}/
  .body
    = form_with model: @post ,local: true do |f|
      .form__image
        = f.fields_for :images do |i|
          = i.file_field :src
      .form__name
        = f.text_field :name
      .form__button
        = f.submit